{
  "version": "1.7.3",
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": "src/",
    "outDir": "dist/",
    "sourceMap": true,
    "target": "es5",
    "module": "amd",
    "declaration": false,
    "noImplicitAny": true,
    "noResolve": true,
    "removeComments": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "classic"
  },
  "exclude": [
    "node_modules"
  ]
}
